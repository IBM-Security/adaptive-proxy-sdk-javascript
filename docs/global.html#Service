    <h4 class="name" id="Service"><span class="type-signature"></span>new Service<span class="signature">(auth, baseURL, context, contentTypeHeader<span class="signature-attributes">opt</span>, acceptHeader<span class="signature-attributes">opt</span>)</span><span class="type-signature"></span></h4>

    



